<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Theme">
        <attr name="disableableButtonTextColor" format="color" />
        <!-- Spinner with the popup style -->
        <attr name="popupSpinnerStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="IconProgressBar">
        <attr name="indeterminateIconName" format="string" />
        <attr name="indeterminateIconColor" format="color" />
        <attr name="indeterminateIconPulse" format="boolean" />
    </declare-styleable>

    <!-- TTA attributes start -->

    <declare-styleable name="FormEditText">
        <attr name="android:inputType" />
        <attr name="android:text" />
        <attr name="android:hint" />
    </declare-styleable>

    <!-- TTA attributes end -->
</resources>
